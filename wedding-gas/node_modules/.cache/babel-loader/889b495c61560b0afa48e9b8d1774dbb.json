,EAAA,IAAA,EAAA;AAAA,WAAA;AACA;AACA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,SAAA,EAAA,EAHA;AAIA,MAAA,KAAA,EAAA,EAJA;AAKA,MAAA,MAAA,EAAA,IALA;AAOA,MAAA,OAAA,EAAA,EAPA;AAQA,MAAA,OAAA,EAAA,EARA;AAUA,MAAA,YAAA,EAAA,EAVA;AAWA,MAAA,IAAA,EAAA,wDAXA;AAYA,MAAA,QAAA,EAAA,+CAZA;AAcA,MAAA,KAAA,EAAA,CAAA,GAAA,EAAA,GAAA,EAAA,GAAA,EAAA,GAAA,EAAA,GAAA,CAdA;AAgBA,MAAA,eAAA,EAAA,IAhBA;AAiBA,MAAA,SAAA,EAAA,IAjBA;AAmBA,MAAA,YAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,CAAA,IAAA,EAAA,IAAA,4CAAA;AAAA,OADA,CAEA;AAFA,OAnBA;AAuBA,MAAA,MAAA,EAAA;AAvBA,KAAA;AAAA,GARA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,QAAA;AAAA,8EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,GAAA,CAAA,QADA;;AAAA;AACA,gBAAA,CADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AAFA,iDAGA,CAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OADA;AAOA,IAAA,UAAA;AAAA,gFAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,YADA,GACA;AACA,kBAAA,QAAA,EAAA,KAAA,QADA;AAEA,kBAAA,SAAA,EAAA,KAAA,SAFA;AAGA,kBAAA,KAAA,EAAA,KAAA,KAHA;AAIA,kBAAA,MAAA,EAAA,QAAA,CAAA,KAAA,MAAA;AAJA,iBADA;AAQA,gBAAA,KAAA,CACA,IADA,qDACA,YADA,EAEA,IAFA,GAGA,KAHA,CAGA,UAAA,KAAA,EAAA;AACA,kBAAA,KAAA,CAAA,eAAA,GAAA,KAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,iBANA;AAQA,qBAAA,eAAA,GAAA,IAAA;AACA,qBAAA,OAAA,GACA,YAAA,CAAA,SAAA,GAAA,sCADA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAPA,GAlCA;AA+DA,EAAA,OA/DA,qBA+DA;AACA,SAAA,QAAA;AACA;AAjEA,CAAA","sourcesContent":["<template >\n  <v-container class=\"formulaire\" >\n    <v-row class=\"text-center\">\n      <v-col class=\"mb-5\" cols=\"12\">\n        <template>\n          <v-dialog v-model=\"dialog\" persistent max-width=\"310\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <validation-observer ref=\"observer\" v-slot=\"{ invalid }\">\n                <v-form class=\"Form\" @submit.prevent=\"createData\">\n                  <validation-provider\n                    v-slot=\"{ errors }\"\n                    name=\"nom\"\n                    rules=\"required\"\n                  >\n                    <v-text-field\n                      v-model=\"lastName\"\n                      label=\"Nom\"\n                      name=\"lastName\"\n                      required\n                      :error-messages=\"errors\"\n                    ></v-text-field>\n                  </validation-provider>\n\n                  <validation-provider\n                    v-slot=\"{ errors }\"\n                    name=\"prénom\"\n                    rules=\"required\"\n                  >\n                    <v-text-field\n                      v-model=\"firstName\"\n                      label=\"Prénom des personnes séparés par une virgule\"\n                      name=\"firstName\"\n                      required\n                      :error-messages=\"errors\"\n                    ></v-text-field>\n                  </validation-provider>\n\n                  <validation-provider\n                    v-slot=\"{ errors }\"\n                    name=\"email\"\n                    rules=\"required|email\"\n                  >\n                    <v-text-field\n                      v-model=\"email\"\n                      label=\"Email\"\n                      name=\"email\"\n                      required\n                      :error-messages=\"errors\"\n                    ></v-text-field>\n                  </validation-provider>\n\n                  <validation-provider\n                    v-slot=\"{ errors }\"\n                    name=\"nombre de personnes\"\n                    rules=\"required\"\n                  >\n                    <v-select\n                      v-model=\"number\"\n                      label=\"Nombre de personnes\"\n                      name=\"number\"\n                      :items=\"items\"\n                      required\n                      :error-messages=\"errors\"\n                    ></v-select>\n                  </validation-provider>\n\n                  <v-btn\n                    type=\"submit\"\n                    color=\"success\"\n                    v-bind=\"attrs\"\n                    v-on=\"on\"\n                    :disabled=\"invalid\"\n                    >Envoyer</v-btn\n                  >\n                </v-form>\n              </validation-observer>\n\n              <v-col>\n                <p style=\"color:blue;\" v-if=\"responseSuccess\">\n                  {{ Message }}\n                </p>\n                <p style=\"color: red\" v-else>\n                  {{ message }}\n                </p>\n              </v-col>\n            </template>\n\n            <div>\n              <v-card v-if=\"responseSuccess\">\n                <v-toolbar class=\"Respon\" color=\"pink lighten-1\" dark>Success</v-toolbar>\n                <v-card-text style=\"background-color:white;\"\n                  >{{ Message }}\n                </v-card-text>\n                <v-card-actions style=\"background-color:white;\">\n                  <v-spacer></v-spacer>\n                  <v-btn\n                    onClick=\"window.location.reload();\"\n                    style=\"background-color:pink\"\n                    color=\"white\"\n                    text\n                    @click=\"dialog = false\"\n                  >\n                    Ok\n                  </v-btn>\n                </v-card-actions>\n              </v-card>\n\n              <v-card v-else>\n                <v-toolbar class=\"Respon\" color=\"red lighten-1\" dark>Error</v-toolbar>\n                <v-card-text style=\"background-color:white;\"\n                  >{{ message }} <br />\n                  Veuillez réassayer</v-card-text\n                >\n                <v-card-actions style=\"background-color:white;\">\n                  <v-spacer></v-spacer>\n                  <v-btn class=\"envoyez\"\n                    style=\"background-color:red\"\n                    color=\"white\"\n                    text\n                    @click=\"dialog = false\"\n                  >\n                    Retour\n                  </v-btn>\n                </v-card-actions>\n              </v-card>\n            </div>\n          </v-dialog>\n        </template>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<style lang=\"scss\">\n@import url('https://fonts.googleapis.com/css2?family=Raleway&display=swap');\n/*@import url('../assets/fonts/Winter Creative.ttf');\n@import url('../assets/fonts/Yessy.otf');*/\n    .v-card__actions{\n      background: rgb(145,17,151);\n      background: linear-gradient(180deg, rgba(145,17,151,0.20772058823529416) 0%, rgba(250,231,5,0.23012955182072825) 100%);\n    }\n    .formulaire{\n      background: rgb(145,17,151);\n      background: linear-gradient(0deg, rgba(145,17,151,0.20772058823529416) 0%, rgba(205,80,127,0.22172619047619047) 20%, rgba(247,100,41,0.23573179271708689) 35%, rgba(250,231,5,0.23012955182072825) 100%);\n    }\n\n    .theme--light.v-btn:not(.v-btn--flat):not(.v-btn--text):not(.v-btn--outlined) {\n      background-color: #600D5E !important;\n    }\n    .theme--light.v-btn:not(.v-btn--flat):not(.v-btn--text):not(.v-btn--outlined) {\n      background-color: #600D5E !important;\n    }\n    .theme--light.v-btn:not(.v-btn--flat):not(.v-btn--text):not(.v-btn--outlined) {\n      background-color: #600D5E !important;\n    }\n    .titre--vcard{\n      font-family: 'Winter Creative', cursive;\n      font-size: 40px;\n    }\n    .envoyez{\n      background-color: #600D5E !important;\n      color: #fff !important;\n        .success{\n           background-color: #600D5E !important;\n        }\n    }\n    .v-btn:before {\n      background-color: #600D5E !important;\n    }\n</style>\n\n<script>\nimport api from \"../service/apiService\";\n\nimport { required, digits, email, max, regex } from \"vee-validate/dist/rules\";\nimport {\n  extend,\n  ValidationObserver,\n  ValidationProvider,\n  setInteractionMode,\n} from \"vee-validate\";\nimport axios from \"axios\";\n\nsetInteractionMode(\"eager\");\n\nextend(\"digits\", {\n  ...digits,\n  message: \"{_field_} needs to be {length} digits. ({_value_})\",\n});\n\nextend(\"required\", {\n  ...required,\n  message: \"{_field_} est obligatoire\",\n});\n\nextend(\"max\", {\n  ...max,\n  message: \"{_field_} may not be greater than {length} characters\",\n});\n\nextend(\"regex\", {\n  ...regex,\n  message: \"{_field_} {_value_} does not match {regex}\",\n});\n\nextend(\"email\", {\n  ...email,\n  message: \"l'email doit être valide\",\n});\n\nexport default {\n  name: \"Formulaire\",\n\n  components: {\n    ValidationProvider,\n    ValidationObserver,\n  },\n\n  data: () => ({\n    // Data\n    lastName: \"\",\n    firstName: \"\",\n    email: \"\",\n    number: null,\n\n    message: \"\",\n    Message: \"\",\n\n    registedData: 15,\n    Text: \"Il n'y a plus de place, mais vous pouvez vous inscrire\",\n    Text_two: \"Si une place si libère nous vous contacterons\",\n\n    items: [\"1\", \"2\", \"3\", \"4\", \"5\"],\n\n    responseSuccess: true,\n    countdata: true,\n\n    ruleForPhone: [\n      (v) => (v && v >= 10) || \"Le numéro doit être d'au moins 10 chiffres\",\n      //(v) => (v && v <= 16) || \"Le numéro ne doit pas dépasser 16 chiffres\",\n    ],\n    dialog: false,\n  }),\n\n  methods: {\n    readData: async function() {\n      const n = await api.readData;\n      console.log(n);\n      return n;\n    },\n\n    createData: async function() {\n      const resquestData = {\n        lastName: this.lastName,\n        firstName: this.firstName,\n        email: this.email,\n        number: parseInt(this.number),\n      };\n\n      axios\n        .post(`https://nonochris.herokuapp.com/api/wedNonoChris`, resquestData)\n        .then()\n        .catch((error) => {\n          this.responseSuccess = false;\n          this.message = error.response.data.email;\n        });\n\n      this.responseSuccess = true;\n      this.Message =\n        resquestData.firstName + \", votre inscription a été enregistré\";\n    },\n  },\n\n  mounted() {\n    this.readData();\n  },\n};\n</script>\n\n<style>\n.Form {\n  border-style: ridge;\n  border-color: rgb(255, 213, 159);\n  padding-left: 15px;\n  padding-right: 15px;\n  padding-bottom: 15px;\n  margin-left: auto;\n  margin-right: auto;\n  width: 80%;\n}\n\nbutton {\n  width: 50%;\n}\n\n.Row {\n  width: 100%;\n}\n\nul {\n  text-align: center;\n}\n\n.Respon {\n  padding-top:8%;\n}\n\n@media  (max-width: 767px) {\n  .Respon {\n    padding-top: 0%\n  }\n}\n</style>"],"sourceRoot":"src/components"}]}