    },
    "devDependencies": {
        "@vue/cli-plugin-typescript": "^4.5.8",
        "@vue/cli-service": "^4.5.8",
        "sass": "^1.19.0",
        "sass-loader": "^8.0.0",
        "typescript": "~3.5.3",
        "vue": "^2.6.10",
        "vue-cli-plugin-vuetify": "^2.0.7",
        "vue-template-compiler": "^2.6.11",
        "vuetify": "^2.3.17",
        "vuetify-loader": "^1.6.0",
        "webpack-bundle-analyzer": "^4.1.0"
    }
}
